{
	"manifest_version": 2,
	"name": "Jobcom2cal",
	"version": "0.0.1",

	"description": "Jobcom画面にGoogle Calendarに登録するボタンを設置します。",

	"permissions": [
		"tabs",
		"identity",
		"http://ita.cc.*.ac.jp/emp/project/project_shift.php"
		"http://www.google.com/*",
		"https://www.google.com/*",
		"https://www.googleapis.com/*",
		"https://accounts.google.com/o/oauth2/token"
	],

	"content_scripts": [
		{
			"mataches": ["http://ita.cc.*.ac.jp/emp/project/project_shift.php"],
			"js": ["main.js"]
		}
	]
}
